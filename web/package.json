{
	"name": "web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"test:types": "tsc --noEmit",
		"schema:generate": "openapi-typescript http://localhost:8080/v3/api-docs -o src/lib/api/elp.ts --enum --immutable"
	},
	"devDependencies": {
		"@iconify-json/icon-park-outline": "^1.1.15",
		"@iconify-json/material-symbols": "^1.1.76",
		"@iconify-json/material-symbols-light": "^1.1.14",
		"@iconify-json/mdi": "^1.1.64",
		"@iconify-json/uil": "^1.1.8",
		"@playwright/test": "^1.28.1",
		"@sveltejs/kit": "^2.8.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.1",
		"@types/eventsource": "^1.1.15",
		"@types/node": "^22.13.11",
		"@types/tabulator-tables": "^6.2.3",
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"chota": "^0.9.2",
		"cpy-cli": "^5.0.0",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"openapi-typescript": "^7.4.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.1.9",
		"svelte-check": "^4.0.5",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"unplugin-icons": "^0.20.0",
		"vite": "^5.4.4",
		"vitest": "^2.1.4"
	},
	"type": "module",
	"dependencies": {
		"eventsource": "^2.0.2",
		"immutable": "^4.3.5",
		"openapi-fetch": "^0.13.0",
		"tabulator-tables": "^6.0.1"
	}
}
